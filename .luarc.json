{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "Lua.diagnostics.globals": [
    "exports",
    "GetResourceState",
    "GetCurrentResourceName",
    "TriggerEvent",
    "TriggerClientEvent",
    "TriggerServerEvent",
    "RegisterNetEvent",
    "RegisterServerEvent",
    "RegisterCommand",
    "AddEventHandler",
    "CreateThread",
    "Citizen",
    "Wait",
    "source",
    "MySQL",
    "json",
    "fx_version",
    "game",
    "name",
    "author",
    "description",
    "version",
    "dependencies",
    "shared_scripts",
    "client_scripts",
    "server_scripts",
    "ui_page",
    "files",
    "lua54",
    "use_experimental_fxv2_oal",
    "ESX",
    "QBCore",
    "QBX",
    "Framework",
    "PhoneApps"
  ],
  "Lua.diagnostics.disable": [
    "undefined-global",
    "need-check-nil",
    "undefined-field",
    "lowercase-global",
    "inject-field",
    "assign-type-mismatch",
    "param-type-mismatch",
    "return-type-mismatch"
  ],
  "Lua.workspace.library": [
    "${3rd}/luassert/library",
    "${3rd}/busted/library"
  ],
  "Lua.workspace.checkThirdParty": false,
  "Lua.completion.callSnippet": "Both",
  "Lua.completion.keywordSnippet": "Both"
}
